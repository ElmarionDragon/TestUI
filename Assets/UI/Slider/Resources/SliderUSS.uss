.slider-window {
    width: 300px;
    height: 200px;
    background-color: rgb(202, 186, 255);
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 30px;
    border-radius: 15px;
}

.slider {
    width: 200px;
}

.slider-text {
    width: 200px;
    margin-top: 10px;  
}

.slider-text Label {
    color: rgb(0,0,0);
}

.slider-text #unity-text-input {
    color: rgb(0,0,0);
    background-color: rgb(255,255,255);
}
